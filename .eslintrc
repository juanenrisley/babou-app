{
  "parser": "babel-eslint",
  "extends": "react",
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "comma-dangle": ["error", "only-multiline"],
    "indent": [
      "error",
      2
    ],
    "space-before-function-paren": "off"
  }
}
